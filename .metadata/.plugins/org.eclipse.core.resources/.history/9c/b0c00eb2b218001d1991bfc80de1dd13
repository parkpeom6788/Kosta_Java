package step9_test;

import java.util.Random;
import java.util.TreeSet;
public class LottoService {
	public TreeSet<Integer> createLottoNumber() {
		TreeSet<Integer> set = new TreeSet<Integer>();
		while(set.size() < 6) {
			int number = (int)(Math.random()*45)+1;
			set.add(number);
		}
		return set;
	}
}
