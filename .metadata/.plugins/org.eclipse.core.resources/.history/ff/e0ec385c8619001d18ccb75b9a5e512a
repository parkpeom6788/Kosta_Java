package step5.test2;

// VO 클래스
public class Product {
	private int number;
	private String name;
	private String maker;
	private int price;
	
	public Product() {}
	public Product(int number, String name, String maker, int price) {
		this.number = number;
		this.name = name;
		this.maker = maker;
		this.price = price;
	}
	
}
