package step5.test;

public class Product {
	private int number;
	private String name;
	private String maker;
	private int price;
	
	public Product(int number, String name, String maker, int price) {
		super();
		this.number = number;
		this.name = name;
		this.maker = maker;
		this.price = price;
	}
	
	
}
